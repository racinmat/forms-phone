{
	"name": "azathoth/forms-phone",
	"description": "Small modification of Nella Phone control for Nette Forms.",
	"homepage": "http://doc.nellafw.org/forms/phone",
	"type": "library",
	"keywords": ["nette", "forms", "control", "phone", "input"],
	"license": "MIT",
	"authors": [
	{
		"name": "Patrik Votoček",
		"email": "patrik@votocek.cz",
		"homepage": "http://patrik.votocek.cz",
		"role": "Developer"
	},
    {
        "name": "Matěj Račinský"
    }
    ],
	"support": {
		"issues": "https://github.com/nella/forms-phone/issues",
		"source": "https://github.com/nella/forms-phone"
	},
	"autoload": {
		"psr-4": {
			"Nella\\Forms\\Controls\\": "src/Nella/Forms/Controls"
		}
	},
	"require": {
		"php": ">=5.3.1",
		"nette/forms": "~2.3,<2.4.0"
	},
	"require-dev": {
		"nette/tester": "~1.3.0",
		"jakub-onderka/php-parallel-lint": "~0.8.0",
		"jakub-onderka/php-console-highlighter": "~0.3.1"
	},
	"suggest": {
		"latte/latte": "For manual rendering support"
	}
}
